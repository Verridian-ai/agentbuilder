# Agent Builder Frontend Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# API CONFIGURATION
# =============================================================================
# Backend API base URL (leave empty for local simulation mode)
VITE_API_BASE=http://localhost:8080

# Supabase URL (optional - for Supabase function calls)
# VITE_SUPABASE_URL=https://your-project.supabase.co

# =============================================================================
# REAL-TIME CONNECTION
# =============================================================================
# WebSocket URL for real-time updates
VITE_WS_URL=wss://realtime.supabase.co

# =============================================================================
# FEATURE FLAGS
# =============================================================================
# Enable AI features
VITE_ENABLE_AI=true

# Enable GitHub integration
VITE_ENABLE_GITHUB=true

# Enable Cloud IDE features
VITE_ENABLE_CLOUD_IDE=true
